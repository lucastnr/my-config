{
  "workbench.iconTheme": "symbols",
  "editor.tabSize": 2,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "multiCommand.commands": [
    {
      "command": "multiCommand.up1LineKeepCursor",
      "sequence": [
        {
          "command": "editorScroll",
          "args": {
            "to": "up",
            "by": "line"
          }
        },
        "cursorUp"
      ]
    },
    {
      "command": "multiCommand.down1LineKeepCursor",
      "sequence": [
        {
          "command": "editorScroll",
          "args": {
            "to": "down",
            "by": "line"
          }
        },
        "cursorDown"
      ]
    }
  ],
  "cSpell.userWords": [
    "airtable",
    "calendly",
    "Calendy",
    "clsx",
    "eidl",
    "firestore",
    "Fundings",
    "gtag",
    "helloskip",
    "IIFE",
    "MAILCHIMP",
    "nanostore",
    "nanostores",
    "onboarded",
    "picsum",
    "precheck",
    "roadtest",
    "Sendgrid",
    "shadcn",
    "skipgrants",
    "SUPABASE",
    "tailwindcss",
    "tryghost",
    "twirlo",
    "Upsell",
    "Upsert",
    "Wailist",
    "Waitlist",
    "Waitlisted"
  ],
  "vsicons.dontShowNewVersionMessage": true,
  "bracketPairColorizer.depreciation-notice": false,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "git.confirmSync": false,
  "redhat.telemetry.enabled": true,
  "git.autofetch": true,
  "tabnine.experimentalAutoImports": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "terminal.external.osxExec": "Warp.app",
  "security.workspace.trust.untrustedFiles": "open",
  "editor.accessibilitySupport": "off",
  "[mdx]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "source.fixAll.eslint": true,
  },
  "nxConsole.showNodeVersionOnStartup": false,
  "files.associations": {
    "*.rules": "firerules",
    ".env": "env",
    ".env.local": "env"
  },
  "firestorerules.usePrettierFormatter": true,
  "workbench.preferredHighContrastColorTheme": "Default Dark Modern",
  "workbench.preferredDarkColorTheme": "Dark+ Pure Black (OLED)",
  "workbench.preferredHighContrastLightColorTheme": "GitHub Light High Contrast",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "window.autoDetectColorScheme": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "editor.scrollbar.horizontal": "auto",
  "editor.scrollbar.vertical": "auto",
  "editor.wordWrap": "on",
  "symbols.hidesExplorerArrows": false,
  "window.titleBarStyle": "native",
  "apc.electron": {
    "titleBarStyle": "hiddenInset",
    "visualEffectState": "active"
  },
  "apc.sidebar.titlebar": {
    "height": 32,
    "fontSize": 12
  },
  "apc.stylesheet": {
    ".title-label > h2": "display: none",
    ".editor-actions": "display: none"
  },
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true
  },
  "apc.font.family": "Inter",
  "editor.fontSize": 13,
  "editor.cursorBlinking": "phase",
  "editor.minimap.enabled": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lock*, .npmrc",
    "tailwind.config.*": "tailwind.config*, postcss.config*",
    ".env": ".env*",
    ".env.local": ".env*",
    "next.config*": "next-env.d.ts, vercel.json, next-sitemap.config*",
    "jest.config.*": "jest.preset.*",
    "nx.json": "workspace.json, migrations.json",
    ".eslintrc.*": ".prettier*"
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "shakram02.bash-beautify"
  },
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[env]": {
    "editor.defaultFormatter": "IronGeek.vscode-env"
  },
  "security.promptForLocalFileProtocolHandling": false,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "workbench.colorTheme": "Dark+ Pure Black (OLED)",
  "editor.fontFamily": "Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "tailwind-fold.autoFold": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "[hosts]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "window.zoomLevel": 2,
  "workbench.activityBar.location": "hidden"
}