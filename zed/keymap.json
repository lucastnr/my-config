[
  {
    "context": "Editor",
    "bindings": {
      "shift-alt-f": "editor::Format"
    }
  },

  {
    "bindings": {
      "cmd-shift-g": "git_panel::ToggleFocus",
      "alt-z": "editor::ToggleSoftWrap",
      "ctrl-r": "projects::OpenRecent",
      "cmd-b": "workspace::ToggleRightDock",
      "cmd-shift-c": [
        "task::Spawn",
        {
          "task_name": "Open warp"
        }
      ]
    }
  }
]
